        "@types/dompurify": "^2.0.4",
        "@types/node": "^14.11.2",
        "babel-loader": "^8.2.2",
        "deploy-to-github-pages": "^1.0.0",
        "prettier": "^2.2.1",
        "typedoc": "^0.19.2",
        "typescript": "^4.0.3"
    },
    "dependencies": {
        "@theoparis/node-markdown-parser": "1.0.1",
