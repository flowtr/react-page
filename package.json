    "author": {
        "name": "Theo Paris",
        "email": "theoparisdesigns@gmail.com"
    },
    "repository": {
        "url": "https://github.com/flowtr/react-page"
    },
    "publishConfig": {
        "access": "public",
        "registry": "http://1.srv.toes.tech:4873"
    },
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "license": "MIT",
    "private": false,
    "scripts": {
